#pragma config(Sensor, dgtl1,  UpTouch,        sensorTouch)
#pragma config(Sensor, dgtl2,  DownTouch,      sensorTouch)
#pragma config(Sensor, dgtl6,  BottomSwitch,   sensorTouch)
#pragma config(Sensor, dgtl7,  TopSwitch,      sensorTouch)
#pragma config(Motor,  port2,           Motor,         tmotorServoContinuousRotation, openLoop)
//*!!Code automatically generated by 'ROBOTC' configuration wizard               !!*//

task main()
{
while (1==1)
{
	if(SensorValue[UpTouch] == 1 && SensorValue[TopSwitch] == 0)
	{
		motor(Motor) = 40;
	}
	else if(SensorValue[DownTouch] == 1 && SensorValue[BottomSwitch] == 0)
	{
		motor(Motor) = -40;
	}
	else
	{
		motor(Motor) = 0;
	}
}
}
